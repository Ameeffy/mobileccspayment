{
  "expo": {
    "name": "CCS Payment",
    "slug": "ccspayment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ccsdeptlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ccsdeptlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ccspayment"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e1b7c5b-9407-431e-801c-1e037fed03be"
      }
    }
  }
}